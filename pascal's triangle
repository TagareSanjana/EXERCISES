class Solution:
    def generate(self, numRows: int) -> list[list[int]]:
        triangle = []
        for i in range(numRows):
            current_row = [1] * (i + 1)
            if i >= 2:  
                prev_row = triangle[i - 1]
                for j in range(1, i):
                    current_row[j] = prev_row[j - 1] + prev_row[j]
            triangle.append(current_row)
        return triangle


